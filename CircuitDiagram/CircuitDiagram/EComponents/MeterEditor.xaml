<my:ComponentEditor x:Class="CircuitDiagram.EComponents.MeterEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:my="clr-namespace:CircuitDiagram"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <GroupBox Header="Meter Type" Margin="12" Name="groupBox1">
            <StackPanel Name="stackPanel1">
                <RadioButton Content="Ammeter" Height="16" Name="radAmmeter" />
                <RadioButton Content="Voltmeter" Height="16" Name="radVoltmeter" />
            </StackPanel>
        </GroupBox>
    </Grid>
</my:ComponentEditor>
