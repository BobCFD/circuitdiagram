<my:ComponentEditor x:Class="CircuitDiagram.EComponents.LogicGateEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:my="clr-namespace:CircuitDiagram"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <GroupBox Header="Gate Type" Margin="12" Name="groupBox1">
            <StackPanel Name="stackPanel1">
                <RadioButton Content="AND" Height="16" Name="radLogicAND" />
                <RadioButton Content="NAND" Height="16" Name="radLogicNAND" />
                <RadioButton Content="OR" Height="16" Name="radLogicOR" />
                <RadioButton Content="NOR" Height="16" Name="radLogicNOR" />
                <RadioButton Content="NOT" Height="16" Name="radLogicNOT" />
            </StackPanel>
        </GroupBox>
    </Grid>
</my:ComponentEditor>
