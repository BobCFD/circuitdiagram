<my:ComponentEditor x:Class="CircuitDiagram.EComponents.ResistorEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:my="clr-namespace:CircuitDiagram"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Label Content="Resistance" Height="28" HorizontalAlignment="Left" Margin="12,155,0,0" Name="label1" VerticalAlignment="Top" Width="136" />
        <TextBox Height="23" Margin="168,157,12,0" Name="tbxResistance" VerticalAlignment="Top" />
        <GroupBox Header="Resistor Type" Height="137" Margin="12,12,12,0" Name="gbxResistorType" VerticalAlignment="Top">
            <StackPanel Orientation="Vertical">
                <RadioButton Content="Standard" Height="16" Name="radTypeStandard" />
                <RadioButton Content="Variable" Height="16" Name="radTypeVariable" />
                <RadioButton Content="Potentiometer" Height="16" Name="radTypePotentiometer" />
                <RadioButton Content="Thermistor" Height="16" Name="radTypeThermistor" />
                <RadioButton Content="LDR" Height="16" Name="radTypeLDR" />
            </StackPanel>
        </GroupBox>
    </Grid>
</my:ComponentEditor>
