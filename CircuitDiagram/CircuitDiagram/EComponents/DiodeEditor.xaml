<my:ComponentEditor x:Class="CircuitDiagram.EComponents.DiodeEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:my="clr-namespace:CircuitDiagram"
             xmlns:myc="clr-namespace:CircuitDiagram.EComponents"
             x:TypeArguments="myc:Diode"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <GroupBox Header="Type">
            <Grid>
                <StackPanel>
                    <RadioButton Content="Standard" Height="16" Name="radTypeStandard" Checked="RadioChecked" />
                    <RadioButton Content="Zener" Height="16" Name="radTypeZener" Checked="RadioChecked" />
                    <RadioButton Content="LED" Height="16" Name="radTypeLED" Checked="RadioChecked" />
                    <RadioButton Content="Photodiode" Height="16" Name="radTypePhoto" Checked="RadioChecked" />
                    <RadioButton Content="Diode bridge" Height="16" Name="radTypeBridge" Checked="RadioChecked" />
                </StackPanel>
            </Grid>
        </GroupBox>
    </Grid>
</my:ComponentEditor>
