<my:ComponentEditor x:Class="CircuitDiagram.EComponents.TransistorEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:my="clr-namespace:CircuitDiagram"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <GroupBox Header="Transistor type" Margin="12" Name="groupBox1">
            <StackPanel Name="stackPanel1">
                <RadioButton Content="Bipolar NPN" Height="16" Name="radBipolarNPN" />
                <RadioButton Content="Bipolar PNP" Height="16" Name="radBipolarPNP" />
                <RadioButton Content="MOSFET N-Channel" Height="16" Name="radMosfetN" />
                <RadioButton Content="MOSFET P-Channel" Height="16" Name="radMosfetP" />
            </StackPanel>
        </GroupBox>
    </Grid>
</my:ComponentEditor>
