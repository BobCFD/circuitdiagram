                </tb:ToolboxCategory>
            </tb:Toolbox>
        </ScrollViewer>
        <Grid DockPanel.Dock="Right" Width="170" Background="{DynamicResource WindowBackground}">
            <Grid.RowDefinitions>
                <RowDefinition />
                <RowDefinition Height="40" />
            </Grid.RowDefinitions>
            <Grid Name="propProperties" Grid.Row="0">
                
            </Grid>
            <Grid Grid.Row="1">
                <Grid.RowDefinitions>
                    <RowDefinition Height="5" />
                    <RowDefinition />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition />
                </Grid.ColumnDefinitions>
                <Rectangle Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" Stroke="Gray" StrokeThickness="2" Height="1" Width="160" />
                <Label Grid.Row="1" Grid.Column="0" Content="Zoom" Height="28" Margin="5,0,0,0" Name="lblSliderZoom" MouseDoubleClick="lblSliderZoom_MouseDoubleClick" ToolTip="Double click to reset" />
                <Slider Grid.Row="1" Grid.Column="1" Height="23" Name="sliderZoom" Width="100" Minimum="10" Maximum="90" Value="50" ValueChanged="sliderZoom_ValueChanged" TickPlacement="BottomRight" SmallChange="1" LargeChange="10">
                    <Slider.Ticks>
                        <DoubleCollection>50</DoubleCollection>
                    </Slider.Ticks>
                </Slider>
            </Grid>
        </Grid>
        <ScrollViewer DockPanel.Dock="Right" Name="scrollViewer1" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="#D0D0D0">
            <my:CircuitDisplay SnapsToDevicePixels="True" Width="640" Height="480" x:Name="circuitDisplay" Background="#02FFFFFF" MouseMove="circuitDisplay_MouseMove" ContextMenuOpening="circuitDisplay_ContextMenuOpening" ContextMenuClosing="circuitDisplay_ContextMenuClosing" MouseLeftButtonDown="circuitDisplay_MouseDown" MouseLeftButtonUp="circuitDisplay_MouseUp" MouseLeave="circuitDisplay_MouseLeave">
<my:CircuitDisplay.ContextMenu>
                    <ContextMenu>
                        <MenuItem Header="Flip" Command="my:CustomCommands.FlipComponent" />
                        <MenuItem Header="Delete" Command="my:CustomCommands.DeleteComponent" />
                        <ContextMenu.CommandBindings>
                            <CommandBinding Command="my:CustomCommands.FlipComponent" CanExecute="CommandFlipComponent_CanExecute" Executed="CommandFlipComponent_Executed" />
                            <CommandBinding Command="my:CustomCommands.DeleteComponent" CanExecute="CommandDeleteComponent_CanExecute" Executed="CommandDeleteComponent_Executed" />
                        </ContextMenu.CommandBindings>
                    </ContextMenu>
                </my:CircuitDisplay.ContextMenu>
            </my:CircuitDisplay>
        </ScrollViewer>
    </DockPanel>
    <Window.CommandBindings>
        <CommandBinding Command="ApplicationCommands.Open"  CanExecute="CommandOpen_CanExecute" Executed="CommandOpen_Executed" />
        <CommandBinding Command="ApplicationCommands.Save"  CanExecute="CommandSave_CanExecute" Executed="CommandSave_Executed" />
        <CommandBinding Command="Undo" CanExecute="CommandUndo_CanExecute" Executed="CommandUndo_Executed" />
        <CommandBinding Command="Redo" CanExecute="CommandRedo_CanExecute" Executed="CommandRedo_Executed" />
    </Window.CommandBindings>
</Window>
